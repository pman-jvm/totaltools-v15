#. module: atharva_theme_base
#: model_terms:ir.ui.view,arch_db:atharva_theme_base.pwa_main_events
msgid ""
"'use strict';\n"
"        var base_cache_ver = 'base-cache-1';\n"
"        var assets = ['/','/offline/page'];\n"
"        self.addEventListener('install', function(ev) {\n"
"            self.skipWaiting();\n"
"            ev.waitUntil(\n"
"                caches.open(base_cache_ver).then(cache =&gt;{\n"
"                    cache.addAll(assets);\n"
"                })\n"
"            )\n"
"        });\n"
"        self.addEventListener('activate', function(ev) {\n"
"            ev.waitUntil(\n"
"                caches.keys().then(function(keys) {\n"
"                    return Promise.all(keys.map(function(key) {\n"
"                        if (key !== base_cache_ver) {\n"
"                            return caches.delete(key);\n"
"                        }\n"
"                    }));\n"
"                })\n"
"            );\n"
"        });\n"
"        self.addEventListener('fetch', function(ev) {\n"
"            if(ev.request.method != 'GET') {\n"
"                return false;\n"
"            }\n"
"            var getUrl = new URL(ev.request.url);\n"
"            var ajaxCall = ev.clientId;\n"
"            var cssFiles = getUrl.href.includes('.css');\n"
"            var jsFiles = getUrl.href.includes('.js');\n"
"            var webClient = getUrl.href.includes('/web');\n"
"            var manifest = getUrl.href.includes('/manifest.json');\n"
"            var staticFiles = getUrl.href.includes('/static/src');\n"
"            if(!cssFiles){\n"
"                if(!jsFiles){\n"
"                    if(!webClient){\n"
"                        if(ajaxCall == \"\"){\n"
"                            if(ev.request.method == \"GET\"){\n"
"                                if(!manifest){\n"
"                                    if(!staticFiles){\n"
"                                        ev.respondWith(\n"
"                                            fetch(ev.request).then(function (response) {\n"
"                                                return response;\n"
"                                            })\n"
"                                            .catch(error =&gt; {\n"
"                                                return caches.match('/offline/page');\n"
"                                        }));\n"
"                                    }\n"
"                                }\n"
"                            }\n"
"                        }\n"
"                    }\n"
"                }\n"
"            }\n"
"        });"
msgstr ""

