<?xml version="1.0" encoding="utf-8"?>
<templates>
    <!-- Product Snippet Configuration  -->
    <t t-name="theme_alan.dialog_image_modal">
        <div>
            <div class="as-edit-popup-header">
                <h1>Image Gallery Snippet</h1>
            </div>
            <div class="as-edit-popup-body">
                <div id="as_product_edit_popupContent">
                    <div id="as_product_edit_selection" aria-labelledby="as_product_edit_selection_tab">
                        <button class="as-edit-btn as-edit-btn-gray mb-4 addTabBtn" id="add-btn-tab"><i class="fa fa-plus"></i> Add Tab</button>
                        <div class="as-tab-select-list-view"/>
                    </div>
                </div>
            </div>
            <div class="as-edit-popup-footer">
                <button class="as-edit-btn as-edit-btn-primary as-save-tab-dialog">Save</button>
                <button class="as-edit-btn as-edit-btn-secondary as-close-dialog">Close</button>
            </div>
        </div>
    </t>
</templates>